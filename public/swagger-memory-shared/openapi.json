{
    "openapi": "3.0.0",
    "paths": {
        "/api/users": {
            "get": {
                "tags": [
                    "users"
                ],
                "description": "statusがMEMBERもしくはADMINのユーザ情報をページネーション形式で取得する(件数：50件)",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::index",
                "parameters": [
                    {
                        "name": "name@like",
                        "in": "query",
                        "description": "ユーザ名の検索値(あいまい検索に対応)",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "sort_name",
                        "in": "query",
                        "description": "ユーザ名でソート",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "sort_created_at",
                        "in": "query",
                        "description": "作成日時順でソート",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / statusがMEMBERもしくはADMINのユーザデータを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "statusがMEMBERもしくはADMINのユーザデータを表示",
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/user_list"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を取得出来ませんでした"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            },
            "post": {
                "tags": [
                    "users"
                ],
                "description": "ユーザデータを保存する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::store",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "request": {
                                        "$ref": "#/components/schemas/user_register"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success / 保存成功のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "info_message": {
                                            "description": "保存成功のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を登録しました"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報の登録に失敗しました"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "/api/users/{user}": {
            "get": {
                "tags": [
                    "users"
                ],
                "description": "指定したユーザの情報をすべて取得する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::show",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / 存在するユーザかつステータスがMEMBERもしくはADMINのユーザデータを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/user_detail"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "存在しないユーザのページをリクエストした場合、検索結果が0件であることを表すメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "検索結果が0件であることを表すメッセージを表示",
                                            "type": "string",
                                            "example": "指定したユーザは存在しません"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を取得出来ませんでした"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            },
            "put": {
                "tags": [
                    "users"
                ],
                "description": "ユーザデータを更新保存する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::update",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "request": {
                                        "$ref": "#/components/schemas/user_update"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success / 保存成功のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "info_message": {
                                            "description": "保存成功のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を登録しました"
                                        },
                                        "edituser": {
                                            "$ref": "#/components/schemas/user_update"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報の登録に失敗しました"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            },
            "delete": {
                "tags": [
                    "users"
                ],
                "description": "ユーザデータを論理削除する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::destroy",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / 論理削除成功のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "info_message": {
                                            "description": "論理削除成功のメッセージを表示",
                                            "type": "string",
                                            "example": "退会が完了しました"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "サーバーエラーにより退会に失敗しました。管理者にお問い合わせください"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "api/users/{user}/edit": {
            "get": {
                "tags": [
                    "users"
                ],
                "description": "編集対象のユーザ情報を取得する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::edit",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / 編集するユーザのデータを表示(基本的にはログインしているユーザは自身の情報しか編集できないようにする)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/user_detail"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を取得出来ませんでした"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "/api/validate": {
            "post": {
                "tags": [
                    "users"
                ],
                "description": "ユーザ新規登録時のバリデーションを実行する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::webValidate",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "request": {
                                        "$ref": "#/components/schemas/user_register"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success / バリデーションチェック通過のメッセージをリターン",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "validate_status": {
                                            "description": "バリデーションチェック通過のメッセージをリターン",
                                            "type": "string",
                                            "example": "OK"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Server error / バリデーションエラーのメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "errors": {
                                            "$ref": "#/components/schemas/user_errors"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "api/users/validate": {
            "post": {
                "tags": [
                    "users"
                ],
                "description": "ユーザ更新時のバリデーションを実行する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::userValidate",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "request": {
                                        "$ref": "#/components/schemas/user_update"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success / バリデーションチェック通過のメッセージをリターン",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "validate_status": {
                                            "description": "バリデーションチェック通過のメッセージをリターン",
                                            "type": "string",
                                            "example": "OK"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Server error / バリデーションエラーのメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "errors": {
                                            "$ref": "#/components/schemas/user_errors"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "api/users/{user}/families": {
            "get": {
                "tags": [
                    "users"
                ],
                "description": "同じグループに加盟しているユーザ情報をすべて取得する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::families",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / 同一グループにいる、group_historiesのstatusが2(メンバー)であるユーザの情報を表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "同一グループにいる、group_historiesのstatusが2(メンバー)であるユーザの情報を表示",
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/user_list"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を取得出来ませんでした"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "api/users/{user}/groups": {
            "get": {
                "tags": [
                    "users"
                ],
                "description": "自身が加盟しているグループ情報をすべて取得する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::participating",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / ログインしているユーザの、group_historiesのstatusが2(メンバー)であるグループ情報を表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "ログインしているユーザの、group_historiesのstatusが2(メンバー)であるグループ情報を表示",
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/user_groups"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を取得出来ませんでした"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        },
        "api/users/{user}/messages": {
            "get": {
                "tags": [
                    "users"
                ],
                "description": "自身がトークでメッセージのやり取りをしているすべてのユーザの情報と、それらのユーザに紐づく最新メッセージをすべて取得する",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::messages",
                "parameters": [
                    {
                        "name": "user",
                        "in": "path",
                        "description": "ユーザID",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success / メッセージのやり取りをしているすべてのユーザの情報と、それらのユーザに紐づく最新メッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "メッセージのやり取りをしているすべてのユーザの情報と、それらのユーザに紐づく最新メッセージを表示",
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "content": {
                                                        "type": "string",
                                                        "example": "元気にしてたー？"
                                                    },
                                                    "own_id": {
                                                        "type": "integer",
                                                        "example": 2
                                                    },
                                                    "user_id": {
                                                        "type": "integer",
                                                        "example": 4
                                                    },
                                                    "update_user_id": {
                                                        "type": "integer",
                                                        "example": 2
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "example": "2021-04-25 12:02:55"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "example": "2021-04-28 14:13:00"
                                                    },
                                                    "otherid": {
                                                        "type": "integer",
                                                        "example": 4
                                                    },
                                                    "mcount": {
                                                        "type": "integer",
                                                        "example": 3
                                                    },
                                                    "other": {
                                                        "description": "usersテーブルとのリレーションデータ",
                                                        "required": [
                                                            "id",
                                                            "name",
                                                            "image_file"
                                                        ],
                                                        "properties": {
                                                            "id": {
                                                                "type": "integer",
                                                                "example": "6"
                                                            },
                                                            "name": {
                                                                "type": "string",
                                                                "example": "test user"
                                                            },
                                                            "image_file": {
                                                                "type": "string",
                                                                "example": "xxxxoooo.png"
                                                            }
                                                        },
                                                        "type": "object"
                                                    }
                                                },
                                                "type": "object"
                                            },
                                            "require": [
                                                "id",
                                                "content",
                                                "own_id",
                                                "user_id",
                                                "update_user_id",
                                                "created_at",
                                                "updated_at",
                                                "otherid",
                                                "mcount"
                                            ]
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error / サーバエラー用のメッセージを表示",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error_message": {
                                            "description": "サーバエラー用のメッセージを表示",
                                            "type": "string",
                                            "example": "ユーザ情報を取得出来ませんでした"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "user_list": {
                "required": [
                    "id",
                    "name",
                    "hobby",
                    "gender",
                    "description",
                    "status",
                    "image_file"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "test1"
                    },
                    "hobby": {
                        "type": "string",
                        "example": "野球観戦"
                    },
                    "gender": {
                        "type": "integer",
                        "example": "0"
                    },
                    "description": {
                        "type": "string",
                        "example": "仲良くしましょう！よろしく！"
                    },
                    "status": {
                        "type": "integer",
                        "example": "1"
                    },
                    "image_file": {
                        "type": "string",
                        "example": "xxxxoooo.png"
                    }
                },
                "type": "object"
            },
            "user_detail": {
                "required": [
                    "id",
                    "name",
                    "hobby",
                    "gender",
                    "description",
                    "email",
                    "status",
                    "image_file"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "test1"
                    },
                    "hobby": {
                        "type": "string",
                        "example": "野球観戦"
                    },
                    "gender": {
                        "type": "integer",
                        "example": "0"
                    },
                    "description": {
                        "type": "string",
                        "example": "仲良くしましょう！よろしく！"
                    },
                    "email": {
                        "type": "string",
                        "example": "test1@xxx.co.jp"
                    },
                    "status": {
                        "type": "integer",
                        "example": "1"
                    },
                    "image_file": {
                        "type": "string",
                        "example": "xxxxoooo.png"
                    }
                },
                "type": "object"
            },
            "user_register": {
                "required": [
                    "name",
                    "email",
                    "password",
                    "password_confirmation",
                    "user_agent",
                    "image_file"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "test1"
                    },
